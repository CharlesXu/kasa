choice
	prompt "Interface"
	default CONFIG_SENSOR_IMX322_SPI
	depends on CONFIG_SENSOR_IMX322
	help
		Select the communication interface.

config CONFIG_SENSOR_IMX322_I2C
	bool "I2C"


config CONFIG_SENSOR_IMX322_SPI
	bool "SPI"

endchoice

