config BUILD_AMBARELLA_ORYX_CONFIG
  bool "Build Oryx Configuration Module"
  depends on BUILD_AMBARELLA_ORYX_MW
  select BUILD_AMBARELLA_ORYX_OSAL
  select BUILD_AMBARELLA_ORYX_UTIL
  select CONFIG_AMBARELLA_LUA_SUPPORT
  default n
  help
    Build Oryx Configuration Module

