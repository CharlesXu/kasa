config BUILD_AMBARELLA_ORYX_SERVICE_SYSTEM
  bool "Build Oryx System Service"
  depends on BUILD_AMBARELLA_ORYX_SERVICES
  select BUILD_AMBARELLA_ORYX_UPGRADE
  select BUILD_AMBARELLA_ORYX_LED
  select CONFIG_AMBARELLA_NTP_SUPPORT
  default n
  help
    Build Oryx IPC Service System

