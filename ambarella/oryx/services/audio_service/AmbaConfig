config BUILD_AMBARELLA_ORYX_SERVICE_AUDIO
  bool "Build Oryx Audio Service"
  depends on BUILD_AMBARELLA_ORYX_SERVICES
  select BUILD_AMBARELLA_ORYX_AUDIO
  select BUILD_AMBARELLA_ORYX_AUDIO_DEVICE
  default n
  help
    Build Oryx Audio Service

