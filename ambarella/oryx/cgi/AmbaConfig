config BUILD_AMBARELLA_ORYX_CGI
  bool "Build Oryx CGI"
  depends on BUILD_AMBARELLA_ORYX_SERVICES
  select CONFIG_AMBARELLA_WEB_SERVICE_SUPPORT
  select CONFIG_AMBARELLA_JSONC_SUPPORT
  default n
  help
    Build Oryx CGI

config BUILD_AMBARELLA_ORYX_WEB_APPS
  bool "Build Oryx web apps"
  depends on BUILD_AMBARELLA_ORYX_CGI
  default n
  help
    Build Oryx web apps
