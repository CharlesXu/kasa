config BUILD_AMBARELLA_ORYX_JPEG_SNAPSHOT
  bool "Build Oryx Jpeg Snapshot"
  depends on BUILD_AMBARELLA_ORYX_UTILITY
  select BUILD_AMBARELLA_ORYX_JPEG_ENCODER
  default n
  help
    Build Oryx Jpeg Snapshot for Motion Handle
