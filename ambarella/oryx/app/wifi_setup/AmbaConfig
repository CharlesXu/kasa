config BUILD_AMBARELLA_ORYX_APP_WIFI_SETUP
  bool "Build oryx wifi setup"
  depends on BUILD_AMBARELLA_ORYX_APP
  select BUILD_AMBARELLA_ORYX_ANALYTICS
  select BUILD_AMBARELLA_ORYX_ANALYTICS_QRCODE
  select BUILD_AMBARELLA_ORYX_AUDIO_DEVICE
  select BUILD_AMBARELLA_ORYX_AUDIO_DEVICE_PULSE
  select BUILD_AMBARELLA_ORYX_CONFIG
  select BUILD_AMBARELLA_ORYX_OSAL
  select BUILD_AMBARELLA_ORYX_UTIL
  select BUILD_AMBARELLA_ORYX_STREAM_PLAYBACK
  select CONFIG_AMBARELLA_SYSTEM_SOUND_FILES
  default n
  help
    Build Oryx Wifi Setup Application

